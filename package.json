{
  "name": "react-confetti",
  "version": "2.1.0-development",
  "description": "React component to draw confetti for your party.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alampros/react-confetti.git"
  },
  "keywords": [
    "react",
    "confetti",
    "component",
    "react-component"
  ],
  "author": "Aaron Lampros <alampros@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alampros/react-confetti/issues"
  },
  "homepage": "https://github.com/alampros/react-confetti#readme",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist/",
    "index.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "develop": "rollup -c -w",
    "test": "yarn run lint",
    "clean": "git clean -xfd dist/",
    "cleanall": "git clean -xfd .",
    "lint": "eslint . --ext .js",
    "prepare": "yarn run clean && yarn run lint && yarn run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "0.14.x || ^15.0.1 || ^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-vectron": "^2.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-sorting": "^0.4.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^1.3.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-uglify": "^6.0.2",
    "semantic-release": "^15.1.7"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": "vectron"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/react-confetti.js",
              "label": "Dev JS"
            },
            {
              "path": "dist/react-confetti.min.js",
              "label": "Minified JS"
            },
            {
              "path": "dist/react-confetti.min.js.map",
              "label": "Source map"
            }
          ]
        }
      ]
    ]
  }
}
